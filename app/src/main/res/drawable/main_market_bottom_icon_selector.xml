<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Non focused states -->
    <item android:drawable="@mipmap/ym_market_new" android:state_focused="false" android:state_pressed="false" android:state_selected="false"/>
    <item android:drawable="@mipmap/ym_market_new_selector" android:state_focused="false" android:state_pressed="false" android:state_selected="true"/>
    <!-- Focused states -->
    <item android:drawable="@mipmap/ym_market_new_selector" android:state_focused="true" android:state_pressed="false" android:state_selected="false"/>
    <item android:drawable="@mipmap/ym_market_new_selector" android:state_focused="true" android:state_pressed="false" android:state_selected="true"/>
    <!-- Pressed -->
    <item android:drawable="@mipmap/ym_market_new_selector" android:state_pressed="true" android:state_selected="true"/>
    <item android:drawable="@mipmap/ym_market_new_selector" android:state_pressed="true" />
</selector>